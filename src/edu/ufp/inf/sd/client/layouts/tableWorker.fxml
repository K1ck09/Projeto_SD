<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane  xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.ufp.inf.sd.client.ItemWorkerController">
    <children>
        <HBox fx:id="tableWorker" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="40.0" prefWidth="731.0" styleClass="tableBar" stylesheets="@css/app.css">
            <children>
                <Label fx:id="tableWorkerID" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="40.0" prefWidth="94.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableWorkerOwner" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="127.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableWorkerState" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="119.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableLastMakeSpan" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="150.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableBestMakeSpan" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="150.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableTimesSubmitted" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="150.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableRewarded" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="101.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
            </children>
        </HBox>
        <Button mnemonicParsing="false" onAction="#handlerWorkerSelect" prefHeight="40.0" prefWidth="731.0"
                style="-fx-background-color: transparent; -fx-background-radius: 0;"/>
    </children>
</AnchorPane>
