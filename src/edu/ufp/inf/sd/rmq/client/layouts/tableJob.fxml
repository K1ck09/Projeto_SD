<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.ufp.inf.sd.rmq.client.ItemController">
    <children>
        <HBox fx:id="tableItem" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="40.0" prefWidth="731.0" styleClass="tableBar" stylesheets="@css/app.css">
            <children>
                <Label fx:id="tableJob" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableOwner" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableStrat" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableReward" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableWorkers" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableState" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="88.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableWorkLoad" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="100.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
                <Label fx:id="tableBestResult" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                       prefHeight="94.0" prefWidth="102.0" style="-fx-background-color: transparent;"
                       textAlignment="CENTER" textFill="#363636"/>
            </children>
        </HBox>
        <Button mnemonicParsing="false" onAction="#handlerInsideJob" prefHeight="40.0" prefWidth="731.0"
                style="-fx-background-color: transparent; -fx-background-radius: 0;"/>
    </children>
</AnchorPane>
